 <appSettings>
    <!-- Azure AD settings -->
    <add key="ida:ClientId" value="YOUR_CLIENT_ID" />
    <add key="ida:ClientSecret" value="YOUR_CLIENT_SECRET" />
    <add key="ida:TenantId" value="YOUR_TENANT_ID" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/" />
    <add key="ida:GraphResourceId" value="https://graph.microsoft.com/" /> 
    <add key="ida:RedirectUri" value="http://localhost:44302/" />
  </appSettings>
